    "@tanstack/router-plugin": "^1.121.2",
    "@unchainedsky/react-drawline": "^1.2.1",
    "arktype": "^2.1.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.6",
