{
  "name": "runes-of-the-zenith",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "vite:dev": "vite dev --port 3000 --open",
    "vite:start": "node .output/server/index.mjs",
    "vite:build": "vite build",
    "vite:serve": "vite preview",
    "lint:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:types": "tsc --noEmit",
    "supabase:login": "supabase login",
    "supabase:types": "supabase gen types typescript --project-id kttsshgvhhwzwjhwiesz > src/supabase/databaseTypes.d.ts",
    "db:encounter": "bun run ./scripts/encounterSync.ts",
    "db:enemy": "bun run ./scripts/enemySync.ts",
    "db:map": "bun run ./scripts/mapSync.ts",
    "db:skill": "bun run ./scripts/skillSync.ts",
    "cf:login": "wrangler login",
    "cf:types": "wrangler types --env-interface Env",
    "cf:dev": "wrangler dev",
    "cf:deploy": "npm run vite:build && wrangler deploy"
  },
  "dependencies": {
    "@dnd-kit/helpers": "^0.1.21",
